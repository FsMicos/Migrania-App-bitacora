[run]
source = .
omit = 
    */migrations/*
    */venv/*
    */env/*
    */__pycache__/*
    */node_modules/*
    manage.py
    */settings.py
    */urls.py
    */wsgi.py
    */asgi.py
    */admin.py
    */apps.py
    */tests.py
    */test_*.py
    */*test*.py
    */conftest.py
    */environment.py

[report]
# Regex for lines to exclude from consideration
exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover

    # Don't complain about missing debug-only code:
    def __repr__
    if self\.debug

    # Don't complain if tests don't hit defensive assertion code:
    raise AssertionError
    raise NotImplementedError
    raise ValueError
    raise TypeError
    raise KeyError
    raise AttributeError

    # Don't complain if non-runnable code isn't run:
    if 0:
    if __name__ == .__main__.:
    if TYPE_CHECKING:

    # Don't complain about simple return statements
    return None
    return True
    return False
    return []
    return {}
    return ""

    # Don't complain about simple conditional returns
    return .* if .* else .*

    # Skip abstract methods
    @(abc\.)?abstractmethod

    # Skip pass statements
    pass

ignore_errors = True
skip_covered = False
show_missing = True

[html]
directory = htmlcov

[xml]
output = coverage.xml
