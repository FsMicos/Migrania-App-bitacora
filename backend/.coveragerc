[run]
source = .
omit = 
    */migrations/*
    */venv/*
    */env/*
    */__pycache__/*
    */node_modules/*
    manage.py
    */settings.py
    */urls.py
    */wsgi.py
    */asgi.py
    */admin.py
    */apps.py
    */tests.py
    */test_*.py
    */*test*.py
    */conftest.py
    */environment.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self\.debug
    raise AssertionError
    raise NotImplementedError
    raise ValueError
    raise TypeError
    raise KeyError
    raise AttributeError
    if 0:
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    return None
    return True  
    return False
    pass
    @abstractmethod

ignore_errors = True
show_missing = True

[html]
directory = htmlcov

[xml]
output = coverage.xml
